<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:Gym_Reception_Management_System.ViewModels"
                    xmlns:views="clr-namespace:Gym_Reception_Management_System.Views">
    
    <DataTemplate DataType="{x:Type viewModels:LogInViewModel}">
        <views:LogInView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:SignUpViewModel}">
        <views:SignUpView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type viewModels:CreateMembershipViewModel}">
        <views:CreateMembershipView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type viewModels:MembershipListViewModel}">
        <views:MembershipListView />
    </DataTemplate>
</ResourceDictionary>